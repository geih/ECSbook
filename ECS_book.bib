  country         = {United States},
  issn-linking    = {0270-6474},
  issue           = {11},
  keywords        = {Animals; Basal Ganglia, cytology, physiology; Behavior, Animal, physiology; Cerebral Cortex, cytology, physiology; Electroencephalography; Female; Motor Activity; Neurons, physiology; Rats; Rats, Inbred F344; Rats, Inbred Strains; Scopolamine, pharmacology; Thalamus, cytology, physiology},
  nlm-id          = {8102140},
